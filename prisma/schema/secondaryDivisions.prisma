model SecondaryDivision {
  secondary_division_id String          @id @default(uuid())
  primary_division_id   String
  division_name         String
  latitude              Float
  longitude             Float
  listingsEnd           Listing[]       @relation("ListingEndSecondaryDivision")
  listingsStart         Listing[]       @relation("ListingStartSecondaryDivision")
  primaryDivision       PrimaryDivision @relation(fields: [primary_division_id], references: [primary_division_id])

  @@map("secondary_divisions")
}
